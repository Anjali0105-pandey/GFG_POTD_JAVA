
//User function Template for Java

class Solution
{ 
    static void reverse(Stack<Integer> s)
    {
        // add your code here
        ArrayList<Integer> b=new ArrayList<>();
        while(!s.isEmpty()){
            b.add(s.pop());
        }
        for(int i=0;i<b.size();i++){
            s.push(b.get(i));
        }
    }
}
